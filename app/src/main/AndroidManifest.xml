        <activity android:name="com.DIAPERS.diaper_project.OrgInfoActivity" />
        <activity android:name="com.DIAPERS.diaper_project.IntroduceActivity" />
        <activity android:name="com.DIAPERS.diaper_project.ManagerActivity" />
        <activity android:name="com.DIAPERS.diaper_project.ServiceActivity" />
        <activity android:name="com.DIAPERS.diaper_project.NewsActivity" />
        <activity android:name="com.DIAPERS.diaper_project.EtcActivity" />
        <activity android:name="com.DIAPERS.diaper_project.CntinfoActivity" />
        <activity android:name="com.DIAPERS.diaper_project.UserinfoActivity" />
        <activity android:name="com.DIAPERS.diaper_project.LogModifyActivity" />
        <activity android:name="com.DIAPERS.diaper_project.StatisticActivity" />
        <activity android:name="com.DIAPERS.diaper_project.CntAddActivity" />
        <activity android:name="com.DIAPERS.diaper_project.MainActivity">
        </activity>
        <activity android:name="com.DIAPERS.diaper_project.LoginActivity" />
        <activity android:name="com.DIAPERS.diaper_project.SignUpActivity" />
